<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--数据库链接字符串-->
    <add key="Conn" value="Data Source=192.168.101.254;Initial Catalog=hpdb_base_pjzyy;Persist Security Info=True;User ID=sa;Password=hh123456"/>
    <!--数据库所在地址-->
    <add key="SqlIP" value="192.168.101.254"/>
    <!--仪器型号-->
    <add key="InstrumentName" value="AS-380"/>
    <!--仪器对应DLL名称，由开发提供-->
    <add key="InstrumentDll" value="AS-380"/>
    <!--数据对接方式0是串口 1是网络-->
    <add key="ConnectType" value="1"/>
    <!--Tcp/IP通讯本地IP-->
    <add key="LocalIP" value="127.0.0.1"/>
    <!--Tcp/IP通讯本地端口号 此项不为空时，默认仪器为客户端-->
    <add key="LocalPort" value="5001"/>
    <!--Tcp/IP通讯远程IP-->
    <add key="ServerIP" value="127.0.0.1"/>
    <!--Tcp/IP通讯远程端口号-->
    <add key="ServerPort" value="5001"/>
    <!--串口通讯串口号-->
    <add key="SerialPort" value="COM1"/>
    <!--串口通讯波特率-->
    <add key="BauRate" value="9600"/>
    <!--串口通讯奇偶数据位-->
    <add key="DataBits" value="N"/>
    <!--串口通讯奇偶校验位-->
    <add key="Parity" value="8"/>
    <!--串口通讯停止位-->
    <add key="StopBit" value="1"/>
    </appSettings>
</configuration>