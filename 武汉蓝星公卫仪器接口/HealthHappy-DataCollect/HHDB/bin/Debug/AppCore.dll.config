<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--数据库链接字符串-->
    <add key="Conn" value="Data Source=192.168.101.254;Initial Catalog=hpdb_cqtl;Persist Security Info=True;User ID=sa;Password=hh123456"/>
    <!--数据库所在地址-->
    <add key="SqlIP" value="192.168.101.254"/>
    <!--仪器型号,用于显示-->
    <add key="InstrumentName" value="AS-380"/>
    <!--仪器对应DLL名称，由开发提供。例如AS-380.AS-380TCP,AS-380-->
    <add key="InstrumentDll" value="AS_380.AS380TCP,AS-380"/>
    <!--体检项目编号 仪器所做的项目在体检系统内的项目编码-->
    <add key="GroupId" value="10697"/>
    <!--数据对接方式0是串口 1是网络-->
    <add key="ConnectType" value="1"/>
    <!--Tcp/IP通讯本地IP-->
    <add key="LocalIP" value="192.168.101.213"/>
    <!--Tcp/IP通讯本地端口号-->
    <add key="LocalPort" value="8080"/>
    <!--Tcp/IP通讯远程IP-->
    <add key="ServerIP" value="192.168.101.213"/>
    <!--Tcp/IP通讯远程端口号-->
    <add key="ServerPort" value="8080"/>
    <!--是否使用UDP协议，使用1，不使用0-->
    <add key="IsUDP" value="0"/>
    <!--串口通讯串口号-->
    <add key="SerialPort" value="COM1"/>
    <!--串口通讯波特率-->
    <add key="BauRate" value="9600"/>
    <!--串口通讯奇偶数据位-->
    <add key="DataBits" value="8"/>
    <!--串口通讯奇偶校验位-->
    <add key="Parity" value="None"/>
    <!--串口通讯停止位-->
    <add key="StopBit" value="1"/>
    <!--数据解析定时间隔 单位:秒-->
    <add key="ResultUpdateTime" value="2"/>
  </appSettings>
</configuration>